<h1>All Restaurants</h1>
<ul>
  <% @restaurants.each do |restaurant| %>
    <li><%= restaurant.name %></li>
    <p><%= restaurant.address %><p>
    <%= link_to "Details", restaurant_path(restaurant), class: "btn btn-primary"%>
  <% end %>
</ul>
<%= link_to "Add new restaurant", restaurants_new_path%>
